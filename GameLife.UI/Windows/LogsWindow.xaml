<Window x:Class="GameLife.UI.LogsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GameLife.UI"
        mc:Ignorable="d"
        Title="Логи" Height="450" Width="400" Loaded="Window_Loaded">
    <Grid>
        <DataGrid AutoGenerateColumns="False" x:Name="LogsGrid">
            <DataGrid.Columns>
                <DataGridTextColumn Binding="{Binding StartDate}" Header="StartDate" Width="120"/>
                <DataGridTextColumn Binding="{Binding FinishDate}" Header="FinishDate" Width="125"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
